<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.scyb.aisweather.webservice.bo.Picture" table="PICTURE" schema="WEATHER">
        <property name="picTime">
            <column name="PIC_TIME" sql-type="varchar2" length="32"/>
        </property>
        <property name="picPath">
            <column name="PIC_PATH" sql-type="varchar2" length="200"/>
        </property>
    </class>
</hibernate-mapping>